page{  
  background: #f5f5f5;  
}  

.container {  
  position: absolute;  
  width: 100%;  
  height: 100%;  
  background-color: #fff;  
  color: #939393; 
}

.nav_left {   
  position: absolute;
  top: 0;left: 0;
  width: 25%;  
  height: 100%;   
  background: #f5f5f5;  
  text-align: center;  
}  
 
.nav_left .nav_left_items {   
  height: 40px;  
  line-height: 40px;  
  border-bottom: 1px solid #dedede;  
  font-size: 14px; 
}  
  
.nav_left .nav_left_items.active {  
  background: white;  
  color: rgb(41, 125, 236); 
}  

.nav_right {  
  position: absolute;
  top: 0;
  right: 0;
  flex: 1;
  width: 75%;
  height: 1000px;  
  padding: 10px;  
  box-sizing: border-box;  
  background: #fff;  
}  

.nav_right .nav_right_items {   
  float: left;    
  width: 33.33%;  
  height: 120px;  
  text-align: center;  
}  

.nav_right .nav_right_items image {  
  width: 60px;  
  height: 60px;  
  margin-top: 15px;  
}  

.nav_right .nav_right_items text {  
  display: block;  
  margin-top: 15px;  
  font-size: 14px;  
  color: black; 
  overflow: hidden;  
  white-space: nowrap;  
  text-overflow: ellipsis;  
} 

.nodata_text {
  color: black;
  font-size: 14px;  
  text-align: center;  
} 
