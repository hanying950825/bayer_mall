/* pages/personal/collection/collection.wxss */
.container-collect {
  height: 100%;
}
/* 存在商品列表 */
.container-collect .van-swipe-cell__right {
  background: red;
  color: #fff;
  text-align: center;
  width: 65px;
  font-size: 30rpx;
  line-height: 100px;
}
/* 没有收藏商品 */
.none-collect, .login-status {
  background: #fff;
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
}